                                <div class="dropdown-menu">
                                    <ul>

                                        <li>@Html.ActionLink("About Us", "Hakkimizda")</li>
                                        <li>@Html.ActionLink(" Vision, Mission and Our Values", "Vision")</li>

                                    </ul>
                                </div>
                            </li>
                            <li class="dropdown">
