                                <div class="dropdown-menu">
                                    <ul>

                                        <li>@Html.ActionLink("Printed Labels", "BaskiliEtiket")</li>
                                        <li>@Html.ActionLink("Blank Labels", "BaskisizEtiket")</li>
                                        <li>@Html.ActionLink("Ribbons", "Ribon")</li>

