        <script>
            var dizi = document.getElementsByClassName("foto");
            //for (var i = 0; i < dizi.length; i++) {
            //    dizi[i].style.height = (dizi[i].clientWidth * 0.6).toString() + "px";
            //}

            function myFunction() {

                var dizi = document.getElementsByClassName("foto");
                for (var i = 0; i < dizi.length; i++) {
                    dizi[i].style.height = (dizi[i].clientWidth * 0.6).toString() + "px";
                }
            }
        </script>
